#2 to sort list

num=[10,80 ,35,46,78,23,15,67,9]
num.sort()
print("list in ascending order:",num)

#3 to list out items in dictionary and sort them

dict={ "core","language","functions","programming"}
lst=list(dict)
print(lst)
lst.sort()
print(lst)

#4 to replace first occurence of string 
s=input("Enter String:")
print(s)
s1=s.replace("n","o",1)
print(s1)


#5 to change first letter of the string 

str=input("Enter  The string:")
print("the given string is:",str.title())


#6 tofind repeated items of a list 

num=[10,20,35,25,45,35,25,60,80]
n1=int(input("Enter a number:"))
print(n1,"occurred ",num.count(n1),"times")


#7 to check sum of three elements

a,b,c=[int(x)for x in input("Enter three numbers:").split()]
sum=a+b+c
print(sum)
y=int(input("Enter a number:"))
d=sum/y
print(d)

#8  to find mean median mode
n= [20,90,60] 
n1 = len(n) 
print(n1)
c= sum(n) 
print(c)
mean = c/ n1
print("Mean  is:",mean) 




